{
    "name": "drone718/helpers",
    "type": "project",
    "description": "Collection of Helpers.",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "jbzoo/utils": "*"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    
    "autoload": {
        "psr-4": {
            "drone718\\helpers\\": "src/"
        },
        "files": [
          "State.php"  
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true 
}
